<h1 class="page-title"><%= @board.name %></h1>
<div class="board-pins">
  <ul>
    <li class="pin pin-ad"><%= link_to "upload a new pin", pins_new_path %></li>
    <% @board.pins.each do |pin| %>

      <li class="pin"><%= pin.name %><br><%= image_tag pin.image(:small) %></li>
    <% end %>
  </ul>
</div>